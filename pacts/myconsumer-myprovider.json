{
  "consumer": {
    "name": "MyConsumer"
  },
  "provider": {
    "name": "MyProvider"
  },
  "interactions": [
    {
      "description": "a request for a voucher",
      "providerState": "i have a voucher with a matching id",
      "request": {
        "method": "GET",
        "path": "/vouchers/1234",
        "headers": {
          "Accept": "application/json"
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "promotionId": 1234,
          "primaryProductId": 9999
        }
      }
    },
    {
      "description": "a request for all dogs",
      "providerState": "i have a list of dogs",
      "request": {
        "method": "GET",
        "path": "/dogs",
        "headers": {
          "Accept": "application/json"
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": [
          {
            "dog": 1
          },
          {
            "dog": 2
          }
        ]
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}